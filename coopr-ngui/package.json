{
  "name": "coopr-ngui",
  "version": "1.1.1",
  "description": "Front-end for Coopr",
  "repository": {
    "type": "git",
    "url": "http://github.com/caskdata/coopr.git"
  },
  "scripts": {
    "start": "node ./server.js",
    "build": "npm install && ./node_modules/bower/bin/bower install && node ./node_modules/gulp/bin/gulp.js build",
    "test": "node ./node_modules/karma/bin/karma start test/karma-conf.js",
    "test-single-run": "node ./node_modules/karma/bin/karma start test/karma-conf.js --no-auto-watch --single-run"
  },
  "devDependencies": {
    "bower": "^1.3.10",
    "del": "^0.1.3",
    "gulp": "^3.8.7",
    "gulp-angular-templatecache": "^1.3.0",
    "gulp-autoprefixer": "^1.0.0",
    "gulp-concat": "^2.3.4",
    "gulp-if": "^1.2.4",
    "gulp-jshint": "^1.8.4",
    "gulp-less": "^1.3.3",
    "gulp-livereload": "^2.1.0",
    "gulp-load-plugins": "^0.7.0",
    "gulp-minify-css": "^0.3.8",
    "gulp-ng-annotate": "^0.3.0",
    "gulp-plumber": "^0.6.4",
    "gulp-replace": "^0.4.0",
    "gulp-rev": "^1.1.0",
    "gulp-size": "^1.0.0",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.0",
    "gulp-wrapper": "^0.1.5",
    "karma": "^0.12.21",
    "karma-chrome-launcher": "^0.1.4",
    "karma-jasmine": "^0.1.5",
    "merge-stream": "^0.1.5"
  },
  "dependencies": {
    "cors-anywhere": "^0.2.0",
    "express": "^4.9.7",
    "finalhandler": "^0.3.0",
    "morgan": "^1.3.1",
    "serve-favicon": "^2.1.5"
  }
}
